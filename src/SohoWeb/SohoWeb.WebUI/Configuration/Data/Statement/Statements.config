<?xml version="1.0" encoding="utf-8" ?>
<dataOperations xmlns="http://www.common.com/DataOperation">
  <!--用户流失分析报表-->
  <dataCommand name="CustomersLoseAnalyze" database="MainDB" commandType="Text">
    <commandText>
      <![CDATA[
      SELECT @TotalCount=COUNT(1) FROM [SohoEContentManagement].[dbo].[Customer] WITH(NOLOCK) #StrWhere# 
    
      SELECT * FROM 
      (SELECT TOP (@EndNumber) (ROW_NUMBER() OVER(ORDER BY LogDate ASC)) AS RowNumber
	        ,[CustomerID]
            ,[CusCode]
            ,[TrueName]
            ,[LogDate]
            ,[UserName]
            ,[NickName]
            ,[LastActive]
            ,[CusLevelID]
            ,[CompanyName]
            ,[Address]
            ,[Email]
            ,[Remark]
            ,[VocationID]
            ,[ZipCode]
            ,[ResourceID]
            ,[CityID]
            ,[ProvinceID]
            ,[ComMobile]
            ,[LogTime]
            ,[Sex]
            ,[Birthday]
            ,[isMagazine]
            ,[MinBirthday]
            ,[MarriageStaID]
            ,[FamilyNumID]
            ,[Telephone1]
            ,[Telephone2]
            ,[Telephone3]
            ,[Telephone4]
            ,[Fax1]
            ,[Fax2]
            ,[Fax3]
            ,[Fax4]
            ,[WorkTele1]
            ,[WorkTele2]
            ,[WorkTele3]
            ,[WorkTele4]
            ,[CityZoneID]
            ,[PostStaID]
            ,[AccAmount]
            ,[CountryID]
            ,[BornYear]
            ,[BornMonth]
            ,[BornDay]
            ,[HeadPic]
            ,[MemCardNo]
            ,[GUID]
            ,[CJGUID]
            ,[IsCJ]
            ,[SmallAreaID]
            ,[isActivation]
            ,[BornDate]
            ,[IDCard]
            ,[ApplyShop]
            ,[CardType]
            ,[ImportSheet]
            ,[BrandID]
            ,[CardTypeName]
            ,[WeiBo]
            ,[UserID]
            ,[BookingID]
            ,[AccountNo]
            ,[IdentityCard]
            ,[isVote]
            ,[Career]
            ,[CName]
      FROM [SohoEContentManagement].[dbo].[Customer](NOLOCK) #StrWhere# ) T 
      WHERE RowNumber > @StartNumber
      ]]>
    </commandText>
    <parameters>
    </parameters>
  </dataCommand>
  <!--日销售情况报表-->
  <dataCommand name="DaySalesData" database="MainDB" commandType="Text">
    <commandText>
      <![CDATA[
      SELECT @TotalCount=COUNT(1) FROM [SohoEContentManagement].[dbo].[Orders] WITH(NOLOCK) #StrWhere# 
      
      SELECT @TotalAmnout=ISNULL(SUM([Amount]), 0) FROM [SohoEContentManagement].[dbo].[Orders] WITH(NOLOCK) #StrWhere# 
    
      SELECT * FROM 
      (SELECT TOP (@EndNumber) (ROW_NUMBER() OVER(ORDER BY LogDate DESC)) AS RowNumber
	          ,[OrdersID]
            ,[OrdersCode]
            ,[Amount]
            ,[LogDate]
            ,[Remark]
            ,[AffirmDate]
            ,[DayCode]
            ,[CustomerID]
            ,[ContactorID]
            ,[Description]
            ,[UserID]
            ,[OOS]
            ,[OrderDesc]
            ,[PrintNum]
            ,[PostStaID]
            ,[PaymentKind]
            ,[PaymentDate]
            ,[PaymentCode]
            ,[SongtimeID]
            ,[OrdersStatus]
            ,[PostType]
            ,[DisCount]
            ,[usIntegral]
            ,[InteAmount]
            ,[CarryCharge]
            ,[PaymentAmount]
            ,[CarryTypeID]
            ,[PayStatus]
            ,[usHabiAmount]
            ,[usAccAmount]
            ,[PaymentBankID]
            ,[FapiaoKind]
            ,[FapiaoTaitou]
            ,[FapiaoNeirong]
            ,[ToPayDate]
            ,[isLockStore]
            ,[NetPayID]
            ,[UnionAccountID]
            ,[PostOfficeID]
            ,[TBOrdersStatus]
            ,[PaymentByCODID]
            ,[PreferentialPrice]
            ,[PaymentPostID]
            ,[CheckInfoID]
            ,[isCheck]
            ,[OrderBatchID]
            ,[ReceiveTime]
            ,[CarryTimeID]
            ,[YiChangStatus]
            ,[PeiSongType]
            ,[ReturnAmount]
            ,[CarryGroupID]
            ,[isAlertApp]
            ,[CustomerTempID]
            ,[isOnLine]
            ,[JiajiFei]
            ,[isInvoice]
            ,[BankID]
            ,[PeriodinumID]
      FROM [SohoEContentManagement].[dbo].[Orders](NOLOCK) #StrWhere# ) T 
      WHERE RowNumber > @StartNumber
      ]]>
    </commandText>
    <parameters>
      <param name="@TotalAmnout" dbType="Decimal"  direction="Output" />
    </parameters>
  </dataCommand>
